<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:bslj="http://example.org/ontology/BuildingStandardsLawOfJapan/">

    <owl:Class rdf:about="http://example.org/ontology/BuildingStandardsLawOfJapan/Structure"/>
    <owl:Class rdf:about="http://example.org/ontology/BuildingStandardsLawOfJapan/PartOfBuilding"/>
    <owl:Class rdf:about="http://example.org/ontology/BuildingStandardsLawOfJapan/Facility"/>

    <owl:Class rdf:about="http://example.org/ontology/BuildingStandardsLawOfJapan/Building">
        <rdfs:subClassOf rdf:resource="http://example.org/ontology/BuildingStandardsLawOfJapan/Structure"/>
    </owl:Class>

    <owl:ObjectProperty rdf:about="http://example.org/ontology/BuildingStandardsLawOfJapan/hasPart"/>
    <owl:ObjectProperty rdf:about="http://example.org/ontology/BuildingStandardsLawOfJapan/includes"/>
    <owl:ObjectProperty rdf:about="http://example.org/ontology/BuildingStandardsLawOfJapan/excludes"/>

    <bslj:Building rdf:about="http://example.org/ontology/BuildingStandardsLawOfJapan/BuildingInstance">
        <bslj:hasPart rdf:resource="http://example.org/ontology/BuildingStandardsLawOfJapan/Roof"/>
        <bslj:hasPart rdf:resource="http://example.org/ontology/BuildingStandardsLawOfJapan/Column"/>
        <bslj:hasPart rdf:resource="http://example.org/ontology/BuildingStandardsLawOfJapan/Wall"/>
        <bslj:includes rdf:resource="http://example.org/ontology/BuildingStandardsLawOfJapan/Gate"/>
        <bslj:includes rdf:resource="http://example.org/ontology/BuildingStandardsLawOfJapan/Office"/>
        <bslj:includes rdf:resource="http://example.org/ontology/BuildingStandardsLawOfJapan/Shop"/>
        <bslj:includes rdf:resource="http://example.org/ontology/BuildingStandardsLawOfJapan/Warehouse"/>
        <bslj:excludes rdf:resource="http://example.org/ontology/BuildingStandardsLawOfJapan/RailwaySafetyFacility"/>
        <bslj:excludes rdf:resource="http://example.org/ontology/BuildingStandardsLawOfJapan/StorageTank"/>
    </bslj:Building>

    <bslj:PartOfBuilding rdf:about="http://example.org/ontology/BuildingStandardsLawOfJapan/Roof"/>
    <bslj:PartOfBuilding rdf:about="http://example.org/ontology/BuildingStandardsLawOfJapan/Column"/>
    <bslj:PartOfBuilding rdf:about="http://example.org/ontology/BuildingStandardsLawOfJapan/Wall"/>
    <bslj:Facility rdf:about="http://example.org/ontology/BuildingStandardsLawOfJapan/Gate"/>
    <bslj:Facility rdf:about="http://example.org/ontology/BuildingStandardsLawOfJapan/Office"/>
    <bslj:Facility rdf:about="http://example.org/ontology/BuildingStandardsLawOfJapan/Shop"/>
    <bslj:Facility rdf:about="http://example.org/ontology/BuildingStandardsLawOfJapan/Warehouse"/>
    <bslj:Facility rdf:about="http://example.org/ontology/BuildingStandardsLawOfJapan/RailwaySafetyFacility"/>
    <bslj:Facility rdf:about="http://example.org/ontology/BuildingStandardsLawOfJapan/StorageTank"/>
